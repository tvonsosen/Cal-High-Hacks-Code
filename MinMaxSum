integerList =input("List 5 integers with spaces in between them: ").split()
integerList = list(map(int, integerList))
def miniMaxSum(intList):
    maxInt = max(intList)
    minInt = min(intList)
    listSum = sum(intList)
    minSum = listSum - maxInt
    maxSum = listSum - minInt
    print(minSum,maxSum)
miniMaxSum(integerList)
